@model IEnumerable<Price>
@{
    ViewData["Title"] = "Ana Sayfa";
}
<!-- Hero Section Begin -->
<section class="hero-section">
    <div class="hs-slider owl-carousel">
        <div class="hs-item set-bg" data-setbg="/gymlife/img/hero/hero-1.jpg">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 offset-lg-6">
                        <div class="hi-text">
                            <span>Vücuduna Şekil Ver</span>
                            <h1>GÜÇLÜ <strong>OL</strong> YENİLMEZ <strong>OL</strong></h1>
                            <a href="tel:@ViewData["Telefon"]" class="primary-btn">Bilgi Al</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="hs-item set-bg" data-setbg="/gymlife/img/hero/hero-2.jpg">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 offset-lg-6">
                        <div class="hi-text">
                            <span>Kendini Geliştir</span>
                            <h1>KENDİNİN <strong>EN İYİ VERSİYONU</strong> OL</h1>
                            <a href="tel:@ViewData["Telefon"]" class="primary-btn">Bilgi Al</a>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Hero Section End -->
<!-- ChoseUs Section Begin -->
